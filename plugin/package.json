{
  "name": "capacitor-snackbar-gfc",
  "version": "1.0.0",
  "description": "implement snackbar android",
  "main": "dist/esm/index.js",
  "types": "dist/esm/index.d.ts",
  "scripts": {
    "build": "npm run clean && tsc",
    "clean": "rimraf ./dist",
    "devPublish": "sudo npm run build && npm publish --registry http://localhost:4873",
    "devUnpublish": "sudo npm unpublish capacitor-snackbar-gfc --registry http://localhost:4873 --force",
    "prodPublish": "sudo npm run build && npm publish",
    "ProdUnpublish": "sudo npm unpublish capacitor-snackbar-gfc",
    "verdaccioLogin": "sudo npm login --registry http://localhost:4873",
    "watch": "tsc --watch",
    "prepublishOnly": "npm run build"
  },
  "author": "SAS GeniusFlash Conception",
  "license": "MIT",
  "dependencies": {
    "@capacitor/core": "latest"
  },
  "devDependencies": {
    "rimraf": "^3.0.0",
    "typescript": "^3.2.4",
    "@capacitor/ios": "latest",
    "@capacitor/android": "latest"
  },
  "files": [
    "dist/",
    "ios/",
    "android/",
    "CapacitorSnackbarGfc.podspec"
  ],
  "keywords": [
    "capacitor",
    "plugin",
    "native"
  ],
  "capacitor": {
    "ios": {
      "src": "ios"
    },
    "android": {
      "src": "android"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/SASGeniusFlashConception/capacitor-snackbar-gfc.git"
  },
  "bugs": {
    "url": "https://github.com/SASGeniusFlashConception/capacitor-snackbar-gfc.git/issues"
  }
}
